{
    "name":"First Extension",
    "description":"Shows a menu item",
    "version":"1",
    "permissions": ["contextMenus","activeTab"],
    "background":{
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/*","*://www.youtube.com/*"],
            "js": ["index.js"]
        }
    ],
    "manifest_version": 2
}